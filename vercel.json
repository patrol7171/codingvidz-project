{
	"builds": [
	  {
		"src": "codingvidz-project/wsgi.py",
		"use": "@vercel/python",
		"config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
	  },
	  {
		"src": "build_files.sh",
		"use": "@vercel/static-build",
		"config": {
		  "distDir": "staticfiles_build"
		}
	  }
	],
	"routes": [
	  {
		"src": "/vercel/path0/static/(.*)",
		"dest": "/vercel/path0/static/$1"
	  },
	  {
		"src": "/(.*)",
		"dest": "codingvidz-project/wsgi.py"
	  }
	]
  }