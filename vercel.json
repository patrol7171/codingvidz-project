{	
	"version": 2,
	"builds": [
	  {
		"src": "codingvidz-project/wsgi.py",
		"use": "@vercel/python",
		"config": { 
			"maxLambdaSize": "15mb", 
			"runtime": "python3.9" 
		}
	  },
	  {
		"src": "build_files.sh",
		"use": "@vercel/static",
		"config": {
			"distDir": "staticfiles_build"
		}
	  }
	],
	"routes": [
	  {
		"src": "/(.*)",
		"dest": "codingvidz-project/wsgi.py"
	  }
	]
  }